{
  "name": "rxdb",
  "description": "Offline-first database with Reactive, Sync, Schema, Promises, Mongo-Query, Encryption, LevelDown",
  "version": "1.4.5",
  "author": "pubkey",
  "repository": {
    "type": "git",
    "url": "https://github.com/pubkey/rxdb"
  },
  "keywords": [
    "db",
    "database",
    "offline-first",
    "schema",
    "replication",
    "leveldown",
    "reactive",
    "rxjs",
    "encryption",
    "nosql"
  ],
  "license": "Apache-2.0",
  "main": "./dist/lib/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "pretest": "npm run transpile",
    "test": "gulp --gulpfile ./config/gulpfile.js test && karma start ./config/karma.conf.js",
    "test:node": "npm run pretest && gulp --gulpfile ./config/gulpfile.js test && npm run posttest",
    "test:browser": "npm run pretest && karma start ./config/karma.conf.js",
    "dockertest": "docker run -it -v $(pwd):/usr/src/app markadams/chromium-xvfb-js:latest-onbuild",
    "posttest": "rimraf -rf test_tmp/",
    "lint": "eslint --ignore-path ./config/.eslintignore src test ./config/gulpfile.js",
    "transpile:src": "rimraf -rf dist/lib/ && node node_modules/babel-cli/bin/babel.js src --out-dir dist/lib",
    "transpile:test:helper": "rimraf -rf test_tmp/helper && node node_modules/babel-cli/bin/babel.js test/helper --out-dir test_tmp/helper/",
    "transpile:test:node": "rimraf -rf test_tmp/node && node node_modules/babel-cli/bin/babel.js test/node --out-dir test_tmp/node/",
    "transpile:test:browser": "rimraf -rf test_tmp/browser && node node_modules/babel-cli/bin/babel.js test/browser --out-dir test_tmp/browser",
    "transpile": "concurrently \"npm run transpile:test:helper\" \"npm run transpile:src\" \"npm run transpile:test:node\" \"npm run transpile:test:browser\"",
    "build:rollup": "rollup -c --environment UGLIFY:false --config ./config/rollup.config.js && rollup -c --environment UGLIFY:true --config ./config/rollup.config.js",
    "build:webpack": "webpack --optimize-minimize --config ./config/webpack.config.js",
    "build:browserify": "npm run transpile && browserify dist/lib/browserify.index.js > dist/rxdb.browserify.js && uglifyjs --compress --mangle --output dist/rxdb.browserify.min.js -- dist/rxdb.browserify.js",
    "disc": "npm run transpile && browserify dist/lib/index.js --full-paths | uglifyjs --compress --mangle | discify --open --full-paths",
    "preversion": "npm run lint && npm run test",
    "dev": "watch 'npm test' src/ test/"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-runtime": "6.18.0",
    "clone": "2.1.0",
    "crypto-js": "3.1.8",
    "es6-promise": "4.0.5",
    "jsonschema": "1.1.0",
    "object-path": "0.11.5",
    "pouchdb-core": "6.0.7",
    "pouchdb-find": "0.10.4",
    "random-token": "0.0.8",
    "regexp-clone": "0.0.1",
    "rxjs": "5.0.0-rc.4",
    "sliced": "1.0.1",
    "spark-md5": "0.0.5"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-lodash": "3.2.10",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-preset-stage-0": "6.16.0",
    "babelrc-rollup": "3.0.0",
    "brfs": "1.4.3",
    "browserify": "13.1.1",
    "concurrently": "3.1.0",
    "disc": "1.3.2",
    "eslint": "3.11.0",
    "eslint-plugin-babel": "4.0.0",
    "express": "4.14.0",
    "express-pouchdb": "2.1.0",
    "faker": "3.1.0",
    "gulp": "3.9.1",
    "gulp-mocha": "3.0.1",
    "http-server": "0.9.0",
    "karma": "1.3.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-browserify": "5.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "lodash": "4.17.2",
    "memdown": "1.2.4",
    "mocha": "3.2.0",
    "pouchdb": "6.0.7",
    "pouchdb-adapter-http": "6.0.7",
    "pouchdb-adapter-idb": "6.0.7",
    "pouchdb-adapter-indexeddb": "0.0.0",
    "pouchdb-adapter-leveldb": "6.0.7",
    "pouchdb-adapter-localstorage": "6.0.7",
    "pouchdb-adapter-memory": "6.0.7",
    "pouchdb-adapter-websql": "6.0.7",
    "pouchdb-replication": "6.0.7",
    "pre-commit": "1.1.3",
    "random-int": "1.0.0",
    "request": "2.79.0",
    "request-promise": "4.1.1",
    "request-promise-native": "1.0.3",
    "rimraf": "2.5.4",
    "rollup": "0.36.4",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-commonjs": "5.0.5",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-sizes": "0.2.3",
    "rollup-plugin-uglify": "1.0.1",
    "typescript": "2.0.10",
    "uglify-js": "2.7.4",
    "watch": "1.0.1",
    "watchify": "3.7.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11"
  }
}
